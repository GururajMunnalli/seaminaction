<?xml version="1.0" encoding="UTF-8"?>
<pages xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://jboss.com/products/seam/pages
		http://jboss.com/products/seam/pages-2.0.xsd"
	no-conversation-view-id="/home.xhtml" login-view-id="/login.xhtml">
	<page view-id="*">
		<navigation>
			<rule if-outcome="home">
				<redirect view-id="/mlocs.xhtml" />
			</rule>
			<rule if-outcome="MonitoringLocations">
				<redirect view-id="/mlocs.xhtml">
					<param name="cid" value="0" />
				</redirect>
			</rule>
			<rule if-outcome="Activities">
				<redirect view-id="/acts.xhtml">
					<param name="cid" value="0" />
				</redirect>
			</rule>
			<rule if-outcome="FieldMeasReport">
				<redirect view-id="/report.xhtml">
				<param name="cid" value="0" />
					<param name="type" value="0" />
				</redirect>
			</rule>
			<rule if-outcome="BenthicMeasReport">
				<redirect view-id="/report.xhtml">
				<param name="cid" value="0" />
					<param name="type" value="1" />
				</redirect>
			</rule>
			<rule if-outcome="BenthicMetricsReport">
				<redirect view-id="/report.xhtml">
				<param name="cid" value="0" />
					<param name="type" value="2" />
				</redirect>
			</rule>
		</navigation>
		<navigation from-action="#{identity.logout}">
			<rule>
				<redirect view-id="/login.xhtml" />
			</rule>
		</navigation>
	</page>
</pages>
