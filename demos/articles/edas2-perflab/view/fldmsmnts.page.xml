<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://jboss.com/products/seam/pages
		http://jboss.com/products/seam/pages-2.0.xsd"
	login-required="true">

	<description>Field Measurements (#{results.rowCount}) - Last used: #{currentTime}</description>

	<begin-conversation join="true" flush-mode="manual"/>
	
	<action execute="#{fieldMeasResultListEditor.prepareResults}"/>
	
		<navigation from-action="#{fieldMeasResultListEditor.back()}">
		<rule if-outcome="back">
			<redirect view-id="/acts.xhtml">
				<param name="mlocUid" value="#{param.mlocUid}" />
				<param name="mlocName" value="#{param.mlocName}" />
			</redirect>
		</rule>
	</navigation>

</page>
