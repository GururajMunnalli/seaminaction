<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/MemberRoleList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{memberRoleHome.wire}"/>

   <param name="memberRoleFrom"/>
   <param name="memberRoleId" value="#{memberRoleHome.memberRoleId}"/>


   <navigation from-action="#{memberRoleHome.persist}">
      <rule>
         <end-conversation/>
         <redirect view-id="/MemberRole.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{memberRoleHome.update}">
      <rule>
         <end-conversation/>
         <redirect view-id="/MemberRole.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{memberRoleHome.remove}">
      <rule>
         <end-conversation/>
         <redirect view-id="/MemberRoleList.xhtml"/>
      </rule>
   </navigation>

</page>
