<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://jboss.com/products/seam/pages
		http://jboss.com/products/seam/pages-2.1.xsd">

	<param name="design" value="#{designHome.designId}"/>
	<param name="company"/>

	<restrict>#{s:hasPermission(designHome.definedInstance, 'view')}</restrict>

	<action execute="#{designHome.load}"/>

	<navigation from-action="#{designHome.load}">
		<rule if="#{designHome.definedInstance == null}">
			<redirect view-id="/home.xhtml"/>
		</rule>
	</navigation>

</page>
